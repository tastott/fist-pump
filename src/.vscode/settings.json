// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/*.js": {"when": "$(basename).ts"},
        "**/*?.js": {"when": "$(basename).tsx"},
        "**/*.js.map": true
    },
    "tslint.enable": true,
    "files.trimTrailingWhitespace": true
}